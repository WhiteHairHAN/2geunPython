    "import turtle\n",
    "t = turtle.Turtle()\n",
    "t.shape(\"turtle\")\n",
    "\n",
    "for i in range(6):\n",
    "    t.forward(100)\n",
    "    t.forward(-30)\n",
    "    t.left(60)\n",
    "    t.forward(30)\n",
    "    t.forward(-30)\n",
    "    t.right(120)\n",
    "    t.forward(30)\n",
    "    t.forward(-30)\n",
    "    t.up()\n",
    "    t.goto(0,0)\n",
    "    t.down()\n"
   ]
  }
