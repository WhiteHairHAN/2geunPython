    "PI = 3.141592\n",
    "radius =5\n",
    "def circleArea(radius):\n",
    "    return radius*radius*PI\n",
    "def circleCircumference(radius):\n",
    "    return 2*radius*PI\n",
    "\n",
    "print(\"반지름이 %d인 원의 면적 %f\" %(radius, circleArea(radius)))\n",
    "print(\"반지름이 %d인 원의 둘레 %f\" %(radius, circleCircumference(radius)))"
   ]
